<AnchorPane id="selectDueData" prefHeight="380.0" prefWidth="600.0" style="-fx-background-color: #424242;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="modules.project.controller.myProjectsController">
   <children>
      <TableView fx:id="table" onMouseClicked="#selectProject" layoutX="24.0" layoutY="48.0" prefHeight="268.0" prefWidth="191.0" />
