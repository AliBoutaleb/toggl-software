         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="tableTasks" layoutX="221.0" layoutY="208.0" prefHeight="108.0" prefWidth="364.0" />
      <Label layoutX="229.0" layoutY="169.0" style="-fx-background-color: #FE2E2E;" text="Tasks" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
