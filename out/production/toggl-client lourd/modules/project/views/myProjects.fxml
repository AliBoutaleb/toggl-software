         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button layoutX="24.0" layoutY="327.0" mnemonicParsing="false" onAction="#deleteProject" style="-fx-background-color: red;" text="Delete" textFill="WHITE">
