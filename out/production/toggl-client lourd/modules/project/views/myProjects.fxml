            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="modify" layoutX="107.0" layoutY="327.0" mnemonicParsing="false" onAction="#modifyProject" style="-fx-background-color: orange;" text="Modify" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="save" disable="true" layoutX="195.0" layoutY="327.0" mnemonicParsing="false" onAction="#saveProject" prefHeight="39.0" prefWidth="81.0" style="-fx-background-color: green;" text="Save" textFill="WHITE">
